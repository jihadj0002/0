# ==============================
# Django Project Gitignore
# ==============================

# Local Django database
db.sqlite3

# User uploads
media/
media/*

# Static files generated by collectstatic
staticfiles/
staticfiles/*

# Python cache & environment
__pycache__/
*.pyc
*.pyo
*.pyd
*.pdb
*.pyd
*.so
*.egg-info/
*.dist-info/

# Virtual environment
venv/
.env

# Logs
*.log

# OS files
.DS_Store
Thumbs.db

# IDE/editor stuff
.vscode/
.idea/

# Node / front-end (in case you use a frontend)
node_modules/
npm-debug.log
dist/
build/
